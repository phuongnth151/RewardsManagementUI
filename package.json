{
  "name": "rewardsmanagementui",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.4.0",
    "@testing-library/user-event": "^7.2.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-scripts": "3.2.0",
    "@fortawesome/fontawesome-free": "^5.12.0",
    "babel-polyfill": "^6.26.0",
    "chai": "^4.1.2",
    "fetch": "^1.1.0",
    "npm": "^6.13.6",
    "react-collapsible": "^2.6.2",
    "react-ga": "^2.7.0",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.0",
    "watchman": "^1.0.0",
    "whatwg-fetch": "^3.0.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "jest-coverage": "jest --coverage",
    "test-coverage": "node scripts/test.js --env=jsdom --coverage --runInBand --verbose"
  },
  "devDependencies": {
    "babel-core": "6.25.0",
    "babel-eslint": "10.0.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "8.0.6",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react-app": "9.0.2",
    "babel-register": "6.26.0",
    "babel-runtime": "6.25.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "7.1.0",
    "jest": "24.9.0",
    "jest-cli": "24.9.0",
    "jest-fetch-mock": "^1.6.4",
    "jest-junit": "^3.7.0",
    "npm-run-all": "^4.1.3"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "jest-junit": {
    "suiteName": "jest tests",
    "output": "./reports/unit-test/junit.xml"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/components/**/*.{js}"
    ],
    "testResultsProcessor": "jest-junit",
    "setupFiles": [
      "<rootDir>/config/polyfills.js",
      "<rootDir>/src/setupTests.js"
    ],
    "testMatch": [
      "<rootDir>/src/components/**/*.spec.js",
      "<rootDir>/src/models/*.spec.js",
      "<rootDir>/src/api/*.spec.js"
    ],
    "testEnvironment": "jsdom",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "coverageDirectory": "<rootDir>/reports/coverage",
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js)$"
    ],
    "moduleFileExtensions": [
      "js"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
